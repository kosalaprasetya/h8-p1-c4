<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1.0"
    />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
    />
    <title>This is homepage</title>
  </head>
  <body>
    <header class="bg-info">
      <nav class="navbar container">
        <div class="wrapper">
          <a
            href="/"
            class="btn btn-primary"
          >
            Home
          </a>
          <a
            href="/songs"
            class="btn btn-primary"
          >
            Songs
          </a>
        </div>
      </nav>
    </header>
    <main class="container">
      <h1>Labels</h1>
      <a
        href="/labels/detail"
        class="btn btn-primary"
        >Label Detail</a
      >
      <table class="table">
        <thead>
          <tr>
            <th>ID</th>
            <th>name</th>
            <th>since</th>
            <th>city</th>
          </tr>
        </thead>
        <tbody>
          <% dataLabel.forEach(element => { %>
          <tr>
            <td><%= element.id %></td>
            <td><%= element.name %></td>
            <td><%= element.formattedDate %></td>
            <td><%= element.city %></td>
          </tr>
          <% }) %>
        </tbody>
      </table>
    </main>
    <footer>footer</footer>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
  </body>
</html>
