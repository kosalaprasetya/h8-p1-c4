<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1.0"
    />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
    />
    <title>This is homepage</title>
  </head>
  <body>
    <header class="bg-info">
      <nav class="navbar container">
        <a
          href="/labels"
          class="btn btn-primary"
        >
          Labels
        </a>
      </nav>
    </header>
    <main class="container">
      <h1>Songs</h1>
      <a
        href="/Songs"
        class="btn btn-danger"
        >Return</a
      >
      <div class="output">
        <% data.forEach(element => { %>
        <img
          src="<%= element.imageUrl %>"
          alt="gambar-lagu"
          width="120"
        />
        <div>ID : <%= element.id %></div>
        <div>Title : <%= element.title %></div>
        <div>Band Name : <%= element.bandName %></div>
        <div>Duration : <%= element.duration %></div>
        <div>Genre : <%= element.genre %></div>
        <div>Vote : <%= element.totalVote %></div>
        <a
          href="/songs/<%= element.id %>/upvote"
          class="btn btn-warning"
          >Vote</a
        >
        <div>Date: <%= element.formattedDate %></div>
        <div>Lyric: <%= element.lyric %></div>
        <% }) %>
      </div>
    </main>
    <footer>footer</footer>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
  </body>
</html>
